<style>
    #map {
        height: 300px;
        width: 100%;
    }
</style>
<h3>Google Map </h3>
<div id="map"></div>
<script>
	//https://www.w3schools.com/html/tryit.asp?filename=tryhtml5_geolocation
      function initMap() {
          // var uluru = {lat: -25.363, lng: 131.044};
       var uluru = { lat: 27.6833306, lng: 85.416665 };
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 16,
          center: uluru
        });
        var marker = new google.maps.Marker({
          position: uluru,
          map: map
        });
      }

</script>
<script async defer
        src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBy4hpAV8o3AiIUlFiOAEwNDwehH8JjCFw&callback=initMap">
</script>
